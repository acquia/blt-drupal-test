{
  "name": "acquia/blt-drupal-test",
  "description": "PHPUnit integration for Acquia BLT",
  "license": "GPL-2.0+",
  "authors":[
    {
      "name": "Acquia Engineering",
      "homepage": "https://www.acquia.com",
      "role": "Maintainer"
    }
  ],
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "php": ">=7.4",
    "drupal/core-dev": "^9.0 || ^10.0.0-alpha1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Acquia\\BltDrupalTest\\": "./src/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
